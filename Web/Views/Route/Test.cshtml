@using Web.Models

@model List<TestModel>
@{
    ViewBag.Title = "Test";
}

<h2>Test</h2>
<p id="demo">

    </p>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <h4>VehicleDTO</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @foreach (var str in Model)
        {
            <div class="form-group">
                <div class="col-md-10">
                    @Html.EditorFor(model => str, new { htmlAttributes = new { @class = "form-control" } })
                </div>
            </div>
        }
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" id="save" class="btn btn-default" onclick="location.href='@Url.Action("Test2", "Route")'"/>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="button" value="Create" id="not" class="btn btn-default" onclick="location.href='@Url.Action("Test2", "Route", Model)'"  hidden="hidden"/>
            </div>
        </div>
    </div>
<button type="button"
        onclick="func()">
    a
</button>

}
<script>
    function func() {
        document.getElementById('not').click();
    }
</script>